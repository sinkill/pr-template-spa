include ../content/content
include ../container/container
include ../subtitle/subtitle
include ../heading/heading
include ../col/col
include ../arrow/arrow
include ../fork/fork

+script([], ['index.js'])

mixin index()
	.index
		+content()
			.index__group
				+subtitle()
					| ЧТО ВКЛЮЧЕНО

			+heading()
				| Инструменты:

			.index__group
				+subtitle()
					| СТРУКТУРА ФАЙЛОВ

			+container()
				+col()
					+heading()
						| Исходники

				+col()
					.index__arrow
						+arrow()
							| Компиляция

				+col()
					+heading()
						| Готовые файлы

			+fork()
